<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
            stylesheets="@styles.css">
    <Button text="ONE" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button text="TWO" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Button id="button_three" text="THREE" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
    <Button text="FOUR" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <Button text="FIVE" GridPane.rowIndex="0" GridPane.columnIndex="4"/>
</GridPane>